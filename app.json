{
    "expo": {
        "name": "SleepSpec",
        "slug": "SleepSpec",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "androidStatusBar": {
            "translucent": false,
            "backgroundColor": "#01000F"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.sleepspec.app"
        },
        "android": {
            "package": "com.sleepspec.app",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon-sleepspec.png",
                "backgroundColor": "#01000F"
            },
            "splash": {
                "image": "./assets/images/splash-icon.png",
                "resizeMode": "contain",
                "backgroundColor": "#01000F"
            },
            "permissions": [
                "android.permission.RECORD_AUDIO",
                "android.permission.MODIFY_AUDIO_SETTINGS",
                "android.permission.READ_EXTERNAL_STORAGE",
                "android.permission.WRITE_EXTERNAL_STORAGE"
            ]
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            [
                "expo-font",
                {
                    "assetBundlePatterns": ["**/*"],
                    "extra": {
                        "fonts": {
                            "Poppins-Regular": "assets/fonts/Poppins/Poppins-Regular.ttf",
                            "Poppins-Bold": "assets/fonts/Poppins/Poppins-Bold.ttf",
                            "PublicSans-Regular": "assets/fonts/Public_Sans/PublicSans-Regular.ttf",
                            "PublicSans-Bold": "assets/fonts/Public_Sans/PublicSans-Bold.ttf",
                            "SpaceMono-Regular": "assets/fonts/SpaceMono-Regular.ttf"
                        }
                    }
                }
            ],
            "expo-document-picker",
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "resizeMode": "contain",
                    "backgroundColor": "#01000F"
                }
            ],
            "expo-font",
            "expo-audio",
            "expo-web-browser"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "90ca6acf-5d3a-40f8-80b0-5a2197931e96"
            }
        }
    }
}
