{
    "cli": {
        "version": ">= 15.0.14",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://sleepspec.cloud"
            }
        },
        "preview2": {
            "android": {
                "gradleCommand": ":app:assembleRelease"
            }
        },
        "preview3": {
            "developmentClient": true
        },
        "preview4": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true,
            "android": {
                "buildType": "app-bundle"
            }
            "env": {
                "EXPO_PUBLIC_API_URL": "https://sleepspec.cloud"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
